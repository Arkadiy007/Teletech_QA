        <searchResultsAdditionalFields>CREATED_DATE</searchResultsAdditionalFields>
    </searchLayouts>
    <sharingModel>Private</sharingModel>
    <validationRules>
        <fullName>Check_owner_of_MC</fullName>
        <active>false</active>
        <errorConditionFormula>AND( 
$Setup.EQA_Profiles__c.Validation_Rules__c , 
$User.Id &lt;&gt; PRIORVALUE(OwnerId )
)</errorConditionFormula>
        <errorMessage>Only the owner can update the Monetary Claim.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Dates_Required_Before_Closing</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
  $Setup.EQA_Profiles__c.Validation_Rules_for_MC__c ,
 ISPICKVAL(Claim_Status__c, &apos;Closed&apos;),
OR(
 ISBLANK(Intercompany_Approval_Generated_and_Sent__c), 
 ISBLANK(Form_Approved_By_Country__c ),
 ISBLANK(Form_Approved_by_NEC__c ),
 ISBLANK(Form_Sent_For_Invoice_Credit_Memo__c), 
 ISBLANK(Create_Debit_to_Supplier_Submit_to_AP__c), 
 ISBLANK(Send_Invoice_Credit_Memo_to_Customer__c) 
)
)</errorConditionFormula>
        <errorMessage>All Dates Required before Claim Status can be set to Closed.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>If_Accepted_Nothing_in_Rejected_Amount</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
ISPICKVAL( Supplier_Response__c , &apos;Accept&apos;),
OR(
Amount_Rejected__c &lt;&gt; 0
)
)</errorConditionFormula>
        <errorMessage>Amount rejected must be 0 or blank if Supplier Response is Accept.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>If_Rejected_Nothing_in_Accepted_Amount</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
ISPICKVAL( Supplier_Response__c , &apos;Reject&apos;),
OR(
Amount_Accepted__c &lt;&gt; 0
)
)</errorConditionFormula>
        <errorMessage>Amount Accepted must be 0 or blank if Supplier Response is Reject.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>No_Changes_After_Supplier_Accepts</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
OR(
 ISPICKVAL( Supplier_Response__c , &apos;Accept&apos;),
 ISPICKVAL( Supplier_Response__c , &apos;Partial Acceptance&apos;)
),
OR(
 ISCHANGED(Total_Other__c),
 ISCHANGED(Total_Other_Costs__c), 
 ISCHANGED(Premium_Freight__c), 
 ISCHANGED(Quantity_Sorted__c), 
 ISCHANGED(Sorting_Costs__c), 
 ISCHANGED(Reworked_Costs__c), 
 ISCHANGED(Reworked_Quantity__c), 
 ISCHANGED(No_Good_Material_Costs__c), 
 ISCHANGED(No_Good_Quantity__c), 
 ISCHANGED(Wrong_Part__c), 
 ISCHANGED(Wrong_Part_Costs__c), 
 ISCHANGED(Mixed_Part__c),
 ISCHANGED(Mixed_Part_Costs__c)
)
)</errorConditionFormula>
        <errorMessage>Quantities and Costs can&apos;t be changed if Supplier Response is Accept.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Require_Account_Coding_and_Cost_Center</fullName>
        <active>true</active>
        <errorConditionFormula>AND( Supplier_Code__c  = &apos;2000100&apos;,
$Profile.Id = &apos;00e18000000Ls4c&apos;, 
OR( ISBLANK(Account_Coding__c ),
ISBLANK( Cost_Center__c )
)
)</errorConditionFormula>
        <errorMessage>If Supplier is NNA-SMYRNA/DECHERD/CANTON, Account Coding and Cost Center are required.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Supplier_Response_Can_Not_Change</fullName>
        <active>true</active>
        <errorConditionFormula>AND( 
$Setup.EQA_Profiles__c.Validation_Rules__c , 
ISCHANGED(Supplier_Response__c), 
PRIORVALUE(Supplier_Response__c) &lt;&gt; &apos;&apos; 
)</errorConditionFormula>
        <errorMessage>Supplier Response can not change.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Validate_Total_Supplier_Amount</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
$Profile.Id = &apos;00e18000000Ls4c&apos;,
Total_Supplier_Amount__c &lt;&gt;  Total_Claim_Amount__c
)</errorConditionFormula>
        <errorMessage>Total Supplier Amount must equal the Total Claim Amount.</errorMessage>
    </validationRules>
