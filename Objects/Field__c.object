        <externalId>false</externalId>
        <formula>IF(Formula__c = TRUE,
&quot;Formula&quot;,
CASE( Type__c ,
&quot;boolean&quot;, &quot;Checkbox&quot;,
&quot;currency&quot;, &quot;Currency&quot;,
&quot;date&quot;, &quot;Date&quot;,
&quot;datetime&quot;, &quot;Date/Time&quot;,
&quot;double&quot;, &quot;Formula or Roll-Up Summary&quot;,
&quot;email&quot;, &quot;Email&quot;,
&quot;id&quot;, &quot;Lookup&quot;,
&quot;int&quot;, &quot;Number&quot;,
&quot;multipicklist&quot;, &quot;Picklist (Multi-Select)&quot;,
&quot;phone&quot;, &quot;Phone&quot;,
&quot;picklist&quot;, &quot;Picklist&quot;,
&quot;reference&quot;, &quot;Lookup or Reference&quot;,
&quot;string&quot;, &quot;Text&quot;,
&quot;string (ext id.)&quot;, &quot;Text&quot;,  
&quot;textarea&quot;, &quot;Text Area&quot;,
&quot;url&quot;, &quot;URL&quot;,
&quot;Other&quot;))</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
