    <fields>
        <fullName>Total_Purchase_Price__c</fullName>
        <externalId>false</externalId>
        <formula>IF( RecordType.Name = &apos;Repurchase&apos;, Base_Price__c +  Sales_Tax__c + Inspection_Title_Fee__c + Documentary_Fee__c + Service_Contract__c +  Accessories__c +  Attorney_Fees__c + Other_Purchase_Information__c, Down_Payment__c +  Total_Payments__c  + Attorney_Fees__c + Other_Purchase_Information__c)</formula>
