    <fields>
        <fullName>Total_Repurchase__c</fullName>
        <externalId>false</externalId>
        <formula>IF( RecordType.Name = &apos;Repurchase&apos;, Total_Refund__c, Total_Refund__c +   Lease_Buyout_Payoff__c)</formula>
