    <validationRules>
        <fullName>Require_Other_Deduction_Description</fullName>
        <active>true</active>
        <errorConditionFormula>AND( Other_Deduction_Information__c &gt; 0,  ISBLANK(Other_Deduction_Description__c))</errorConditionFormula>
