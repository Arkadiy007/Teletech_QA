    <validationRules>
        <fullName>Check_if_user_owns_Sorting_Report</fullName>
        <active>false</active>
        <errorConditionFormula>AND( 
$Setup.EQA_Profiles__c.Validation_Rules__c , 
$User.Id &lt;&gt; PRIORVALUE(OwnerId) 
)</errorConditionFormula>
        <errorMessage>Only the owner can update the sorting report.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>RAN_Validation</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
NOT(ISBLANK( Certified_Supplier_RAN_Number__c )),
Last_Suspect_RAN__c  = False,
Future_Shipment_RAN__c = False
)</errorConditionFormula>
        <errorMessage>Last Suspect RAN or Future Shipment RAN must be checked if a Certified Supplier RAN is entered.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Require_NEC_Average_Sort_Time_Per_Part</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
NEC_Sort_Required__c = True,
 ISBLANK(EQA_Average_Sort_Time_Per_Part__c)
)</errorConditionFormula>
        <errorMessage>Average Sort Time Per Part is required if NEC Sort Required is checked.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Supplier_Response_Not_Changeable</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
$Setup.EQA_Profiles__c.Validation_Rules__c ,
ISCHANGED(Supplier_Response__c),
PRIORVALUE(Supplier_Response__c) &lt;&gt; &apos;&apos;
)</errorConditionFormula>
        <errorMessage>Supplier Response can not be changed.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Total_Qty_Sorted_Not_Change_After_Acc</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
OR(
ISPICKVAL( Supplier_Response__c , &apos;Accept&apos;),
ISPICKVAL( Supplier_Response__c , &apos;Partial Acceptance&apos;)
),
OR(
ISCHANGED ( Total_Quantity_Sorted__c ),
ISCHANGED (  Average_Sort_Time_Per_Part__c  )
)
)</errorConditionFormula>
        <errorMessage>Total Quantity Sorted and Average Sort Time Per Part can&apos;t be changed after Supplier Accepts.</errorMessage>
    </validationRules>
