    <fields>
        <fullName>ContractTypeTranslated__c</fullName>
        <description>Translates the VSC Contract Type from the HOST provided value to a term that is clear to users. If VSC Effective Date is before 5/1/12, values are different than after was changed on 5/1/12.</description>
        <externalId>false</externalId>
        <formula>IF( Effective_Date__c &lt; DATEVALUE(&quot;2012-05-01&quot;) &amp;&amp;  OR(Product__c = &quot;SC&quot;, Product__c = &quot;MB&quot;, Product__c = &quot;MC&quot;),
CASE(Contract_Type__c ,
&quot;A&quot;, &quot;New Bronze&quot;,
&quot;B&quot;, &quot;New Silver&quot;,
&quot;C&quot;, &quot;New Gold&quot;,
&quot;L&quot;, &quot;Pre-Owned&quot;,
&quot;N&quot;, &quot;Pre-Owned+Plus&quot;,
&quot;O&quot;, &quot;Premium&quot;,
&quot;Q&quot;, &quot;Schedule 2&quot;,
&quot;T&quot;, &quot;Schedule 1&quot;,
&quot; &quot;)
,
(IF( Effective_Date__c &lt; DATEVALUE(&quot;2012-05-01&quot;) &amp;&amp;  Product__c = &quot;RS&quot;,
CASE(Contract_Type__c ,
&quot;B&quot;, &quot;New Silver Preferred&quot;,
&quot;C&quot;, &quot;New Gold Preferred&quot;,
&quot;M&quot;, &quot;Pre-Owned Preferred&quot;,
&quot; &quot;)
,
(IF( Effective_Date__c  &gt;= DATEVALUE(&quot;2012-05-01&quot;),
CASE(Contract_Type__c ,
&quot;A&quot;, &quot;New Powertrain Preferred&quot;,
&quot;B&quot;, &quot;New Silver Preferred&quot;,
&quot;C&quot;, &quot;New Gold Preferred&quot;,
&quot;G&quot;, &quot;Pre-Owned Gold Preferred&quot;,
&quot;L&quot;, &quot;Pre-Owned Powertrain Preferred&quot;,
&quot;N&quot;, &quot;Pre-Owned Silver Preferred&quot;,
&quot; &quot;)
,
&quot; &quot;
)))))</formula>
        <label>Contract Type</label>
        <required>false</required>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
