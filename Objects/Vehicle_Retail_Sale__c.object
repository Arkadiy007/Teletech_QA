    <webLinks>
        <fullName>Phone_Survey</fullName>
        <availability>online</availability>
        <displayType>link</displayType>
        <linkType>javascript</linkType>
        <masterLabel>Phone Survey</masterLabel>
        <openType>onClickJavaScript</openType>
        <protected>false</protected>
        <url>{!REQUIRESCRIPT(&quot;/soap/ajax/19.0/connection.js&quot;)}; 

var aid = &apos;{!Vehicle_Retail_Sale__c.Id}&apos;;

var queryResults = sforce.connection.query(&quot;select Id, Owner_Id__c, Vehicle_Identification_Number__c, Vehicle_Identification_Number__r.Make_Name__c, Owner_Id__r.PersonContactId from Vehicle_Retail_Sale__c where Id = \&apos;&quot; + aid + &quot;\&apos;&quot;);

var record = queryResults.getArray(&apos;records&apos;)[0]; 

var sidNissan = &apos;{!$Label.SurveyID_VRS_Nissan}&apos;;
var sidInfiniti = &apos;{!$Label.SurveyID_VRS_Infiniti}&apos;;

var make = &apos;&apos;;
if (record.Vehicle_Identification_Number__r != null &amp;&amp; record.Vehicle_Identification_Number__r.Make_Name__c != null)
	make = record.Vehicle_Identification_Number__r.Make_Name__c.toLowerCase();

if (record.Owner_Id__r == null || record.Owner_Id__r.PersonContactId == null)
	alert(&apos;You can not conduct a Phone Survey because there is no Vehicle Owner listed on this Vehicle Retail Sale record.&apos;);
