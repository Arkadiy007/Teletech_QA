    <validationRules>
        <fullName>Can_Not_Edit_Submitted_Apprvd_Denied_CRs</fullName>
        <active>true</active>
        <description>Prevents users from editing a Submitted, Approved, or Denied Check Request</description>
        <errorConditionFormula>OR(
ISPICKVAL(Status__c, &quot;Submitted&quot;),
ISPICKVAL(Status__c, &quot;Approved&quot;),
ISPICKVAL(Status__c, &quot;Denied&quot;),
ISPICKVAL(PRIORVALUE(Status__c), &quot;Submitted&quot;),
ISPICKVAL(PRIORVALUE(Status__c), &quot;Approved&quot;),
ISPICKVAL(PRIORVALUE(Status__c), &quot;Denied&quot;)
)
&amp;&amp;
OR(
ISCHANGED( Check_Amount__c ), 
ISCHANGED( Memo__c ), 
ISCHANGED( GL_Code__c ), 
ISCHANGED( Repair_Order__c ), 
ISCHANGED( RO_Date__c ), 
ISCHANGED( Mileage__c ), 
ISCHANGED( Check_Type__c ),
ISCHANGED( Payable_To_First_Name__c ), 
ISCHANGED( Payable_To_Last_Name__c ), 
ISCHANGED( Business_Name_1__c ),  
ISCHANGED( Business_Name_2__c ),  
ISCHANGED( Address__c ), 
ISCHANGED( City__c ), 
ISCHANGED( State__c ), 
ISCHANGED( Zip__c ), 
ISCHANGED( Mailing_Address_Verified__c )
)</errorConditionFormula>
        <errorMessage>You may not edit a Submitted, Approved, or Denied Check Request</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Check_Amount_Cannot_Equal_Zero</fullName>
        <active>true</active>
        <description>Rule stating the check amount cannot be zero upon saving a check request</description>
        <errorConditionFormula>ISNULL(Check_Amount__c )</errorConditionFormula>
        <errorDisplayField>Check_Amount__c</errorDisplayField>
        <errorMessage>The Check Amount cannot be blank or equal to zero</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Check_Amount_EV_not_greater_than_1k</fullName>
        <active>true</active>
        <description>Can not enter amount greater than $1,000 if User&apos;s Segment = E - Electric Vehicle</description>
        <errorConditionFormula>Check_Amount__c &gt; 1000 &amp;&amp;  ISPICKVAL($User.Segment__c, &apos;E - Electric Vehicle&apos;)</errorConditionFormula>
        <errorDisplayField>Check_Amount__c</errorDisplayField>
        <errorMessage>You can not enter an amount greater than your authorized limit. Please reduce the Check Amount or discuss with your Supervisor.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Check_Amount_Greater_Than_Zero</fullName>
        <active>true</active>
        <description>Requires Check Request Amount field to be greater than zero</description>
        <errorConditionFormula>OR(
Check_Amount__c &lt;= 0 
&amp;&amp;  
AND(
NOT(ISPICKVAL(Status__c , &quot;Adjustment&quot;)),
NOT(ISPICKVAL(Status__c , &quot;Cancelled&quot;))
)
,
ISBLANK(Check_Amount__c)
)</errorConditionFormula>
        <errorDisplayField>Check_Amount__c</errorDisplayField>
        <errorMessage>You MUST enter a value greater than zero</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Check_Amount_not_greater_than_85k</fullName>
        <active>true</active>
        <description>Can not enter amount greater than $85,000</description>
        <errorConditionFormula>Check_Amount__c &gt; 85000</errorConditionFormula>
        <errorDisplayField>Check_Amount__c</errorDisplayField>
        <errorMessage>You can not enter an amount greater than your authorized limit. Please reduce the Check Amount or discuss with your Supervisor.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Check_Recipient_can_not_be_Blank</fullName>
        <active>true</active>
        <description>Requires either the Payable to Last Name or the Business Name 1 field to be populated</description>
        <errorConditionFormula>AND ( 
OR(ISBLANK( Payable_To_First_Name__c ), 
ISBLANK( Payable_To_Last_Name__c )) 
, 
ISBLANK( Business_Name_1__c ) 
)</errorConditionFormula>
        <errorMessage>You MUST enter the check recipient either in the &apos;Payable To&apos; fields or in the &apos;Business Name 1&apos; field</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Failure_Rule_for_NNAETL</fullName>
        <active>false</active>
        <errorConditionFormula>$User.LastName  = &apos;NNAETL&apos;</errorConditionFormula>
        <errorMessage>NNAETL is not allowed to update check requests.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>GL_Code_can_not_be_Blank</fullName>
        <active>true</active>
        <description>Requires a GL code in the check request before submitting for approval.</description>
        <errorConditionFormula>ISBLANK( GL_Code__c )</errorConditionFormula>
        <errorDisplayField>GL_Code__c</errorDisplayField>
        <errorMessage>You MUST enter a GL Code to save this Check Request.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Mail_To_Zip_Minimum_5_Digits</fullName>
        <active>false</active>
        <description>120730 JS: This was deactivated at some point by Dev team because it was preventing of check update from HOST.  

Zip Code must be at least 5 digits.</description>
        <errorConditionFormula>LEN( Zip__c ) &lt; 5</errorConditionFormula>
        <errorDisplayField>Zip__c</errorDisplayField>
        <errorMessage>You must enter at least 5 digits for the Mail to Zip</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Mailing_Address_Verified_Must_Be_Checked</fullName>
        <active>true</active>
        <description>The &apos;Mailing Address Verified&apos; checkbox must be checked</description>
        <errorConditionFormula>Mailing_Address_Verified__c &lt;&gt; TRUE</errorConditionFormula>
        <errorDisplayField>Mailing_Address_Verified__c</errorDisplayField>
        <errorMessage>You MUST verify the Mailing Address and check this box to save this Check Request</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>PC_Zip_Code</fullName>
        <active>true</active>
        <errorConditionFormula>AND( 
NOT(REGEX(PC_Mail_To_Zip__c, &quot;(\\d{5}(-\\d{4})?)?&quot;)), 
ISPICKVAL (PC_Mail_To_Country__c, &apos;US&apos;) 
)</errorConditionFormula>
        <errorMessage>Please enter a Valid PC Zip Code (99999 or 99999-9999).</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>PC_Zip_Code_Canada</fullName>
        <active>true</active>
        <errorConditionFormula>AND( 
ISPICKVAL(PC_Mail_To_Country__c,&apos;CA&apos;), 
NOT(REGEX(PC_Mail_To_Zip__c, &quot;(((?i)[ABCEGHJKLMNPRSTVXY]\\d[A-Z]?\\s?\\d[A-Z]\\d)?)?&quot;)) 
)</errorConditionFormula>
        <errorMessage>Please enter a Valid PC Zip Code (6 alpha-numeric characters).</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Payment_Documents_and_Verification</fullName>
        <active>true</active>
        <description>One of the Payment Verification checkboxes (Payment Documents or Payment Verification) MUST be checked before saving this Check Request.</description>
        <errorConditionFormula>AND( 
Payment_Documents__c &lt;&gt; TRUE 
, 
Payment_Verification__c &lt;&gt; TRUE 
)</errorConditionFormula>
        <errorMessage>One of the Payment Validation checkboxes (Payment Documents or Payment Verification) MUST be checked before saving this Check Request.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>RO_Fields_for_Certain_GL_Codes</fullName>
        <active>false</active>
        <description>130304JS: This was the original Check Request validation rule. Deactivated, can be deleted.

3 Repair Order fields are required for certain GL Codes. For this type of Check Request, you MUST populate the Repair Order, RO Date, and Mileage of RO fields.</description>
        <errorConditionFormula>RIGHT(GL_Code_Description__c , 1) = &quot;Y&quot;
&amp;&amp;
OR(
ISBLANK(Repair_Order__c)
,
ISBLANK( RO_Date__c )
,
ISBLANK( Mileage__c )
)</errorConditionFormula>
        <errorDisplayField>Approval_Indicator__c</errorDisplayField>
        <errorMessage>For this type of Campaign Reimbursement, you must populate all 3 Repair Order fields</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Reimbursement_requires_Fields_Populated</fullName>
        <active>true</active>
        <description>Validation rule is if the Reimbursement flag field is Y then make the population of RO Date, Mileage and RO Number required.</description>
        <errorConditionFormula>Reimbursement_Formula__c = &quot;Y&quot; 
&amp;&amp; 
OR( 
ISBLANK( RO_Date__c ) 
, 
ISBLANK( Mileage__c ) 
, 
ISBLANK( Repair_Order__c ) 
)</errorConditionFormula>
        <errorMessage>For Reimbursements you MUST populate RO #, RO Date, and Mileage of RO</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Vehicle_can_not_be_User_Created</fullName>
        <active>false</active>
        <description>120827: Deactivating this due to CA request along with change in Approval Processes to accommodate this. ***Vehicle records are either system created or User Created.  User Created Vehicle records can not have Check Requests.</description>
        <errorConditionFormula>Case__r.Vehicle_Name__r.RecordTypeId   = &quot;012F0000000yCuR&quot;</errorConditionFormula>
        <errorMessage>You may not submit a Check Request for this Vehicle. Please tell your Team Lead that this Vehicle is not available for Check Requests..</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Vehicle_must_be_Populated</fullName>
        <active>true</active>
        <description>The Vehicle formula field must be populated, requires editing Case to add it if necessary.</description>
        <errorConditionFormula>ISBLANK( Case__r.Vehicle_Name__c )</errorConditionFormula>
        <errorMessage>The related Case must have a Vehicle entered to create this Check Request.  Edit the Case to add the Vehicle, then save the Case and initiate the Check Request again.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Zip_Code</fullName>
        <active>true</active>
        <description>Validates that the Zip/Postal Code is in 99999 or 99999-9999 for US.</description>
        <errorConditionFormula>AND( 
NOT(REGEX(Zip__c, &quot;(\\d{5}(-\\d{4})?)?&quot;)), 
ISPICKVAL (Country__c, &apos;US&apos;) 
)</errorConditionFormula>
        <errorMessage>Please enter a Valid Zip Code (99999 or 99999-9999).</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Zip_Code_Canada</fullName>
        <active>true</active>
