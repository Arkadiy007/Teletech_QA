    <validationRules>
        <fullName>No_Solutions_DPIC_Case_Reasons</fullName>
        <active>true</active>
        <description>Don&apos;t allow solutions to be created for cases with Case Reason of ETA, Stock Check, Key Code</description>
        <errorConditionFormula>OR(ISPICKVAL(Case.Reason , &quot;Stock Check&quot;),ISPICKVAL(Case.Reason , &quot;Backorder ETA&quot;),ISPICKVAL(Case.Reason , &quot;Key Code&quot;))</errorConditionFormula>
        <errorDisplayField>SolutionNote</errorDisplayField>
        <errorMessage>Solutions cannot be created from cases with a Case Reason of ETA, Stock Check or Key Code</errorMessage>
    </validationRules>
